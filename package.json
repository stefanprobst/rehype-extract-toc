{
  "name": "@stefanprobst/rehype-extract-toc",
  "version": "2.0.1",
  "main": "src/index.js",
  "type": "module",
  "types": "src/index.d.ts",
  "repository": "https://github.com/stefanprobst/rehype-extract-toc.git",
  "author": "Stefan Probst <stefanprobst@kolabnow.com>",
  "license": "MIT",
  "engines": {
    "node": "14.x",
    "yarn": "1.x"
  },
  "scripts": {
    "format": "prettier . -c --ignore-path .gitignore",
    "format:fix": "yarn run format --write",
    "lint": "eslint . --ignore-path .gitignore",
    "lint:fix": "yarn run lint --cache --fix",
    "prepare": "simple-git-hooks",
    "test": "NODE_OPTIONS='--experimental-vm-modules' jest"
  },
  "dependencies": {
    "hast-util-heading-rank": "^2.1.0",
    "hast-util-to-string": "^2.0.0",
    "unist-util-visit": "^4.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@stefanprobst/commitlint-config": "^1.0.2",
    "@stefanprobst/eslint-config-node": "^2.0.1",
    "@stefanprobst/prettier-config": "^1.0.5",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.24.0",
    "eslint-plugin-jest": "^24.4.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^27.0.6",
    "lint-staged": "^11.1.2",
    "prettier": "^2.3.2",
    "rehype-parse": "^8.0.2",
    "rehype-slug": "^4.0.1",
    "rehype-stringify": "^9.0.1",
    "simple-git-hooks": "^2.5.1",
    "typescript": "^4.3.5",
    "unified": "^10.1.0"
  },
  "commitlint": {
    "extends": "@stefanprobst/commitlint-config"
  },
  "eslintConfig": {
    "extends": "@stefanprobst/eslint-config-node",
    "ignorePatterns": "*.d.ts",
    "overrides": [
      {
        "files": [
          "*.test.js"
        ],
        "rules": {
          "node/no-unpublished-import": "off"
        }
      }
    ]
  },
  "lint-staged": {
    "*.(md)": "prettier --write",
    "*.(js)": "eslint --cache --fix"
  },
  "prettier": "@stefanprobst/prettier-config",
  "simple-git-hooks": {
    "commit-msg": "yarn commitlint --edit",
    "pre-commit": "yarn lint-staged",
    "pre-push": "yarn run test"
  }
}
